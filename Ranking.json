{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": "Top 10 highest",
  "data": {"url": "Countrydata.csv"},
  "mark": "bar",
  "transform": [
    {
      "aggregate": [{"op": "mean", "field": "suicidemortalityrate", "as": "suicide"}],
      "groupby": ["country"]
    },
    {
      "window": [{"op": "row_number", "as": "rank"}],
      "sort": [{ "field": "suicide", "order": "descending" }]
    },
    {
      "filter": "datum.rank <= 10"
    }
  ],
  "encoding": {
    "x": {
      "aggregate": "mean",
      "field": "suicide",
      "type": "quantitative",
      "title": "Suicide per 100,000 population"
    },
    "y": {
      "sort": {"op": "mean", "field": "suicide", "order": "descending"},
      "field": "country",
      "type": "ordinal",
      "title": null
    }
  }
}